{
	"name": "BildUmschalten"
	, "minimum_chrome_version": "18"
	, "version": "0.3"
	, "manifest_version": 2
	, "content_security_policy": "default-src 'self' "
	, "description": "to enable/disable images, with a shortcut button. It uses experimental apis !!"
	, "permissions": ["tabs", "*://*/*", "experimental", "webRequest", "webRequestBlocking"]
	, "background_page": "background.html"
	, "browser_action": {
		"default_icon": "disallow.png"
	}
	, "background_page": "background.html"
	, "offline_enabled": false
}
